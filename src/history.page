p.content
    h1 < "LGBT History"
    div#tabnav < ul#tabnav
        li < a
            href: "#"
            onclick: "openTab( "Timeline" )"
            "Timeline"
        li < a
            href: "#"
            onclick: "openTab( "Events" )"
            "Events"
        li < a
            href: "#"
            onclick: "openTab( "Organizations" )"
            "Organizations"
        li < a
            href: "#"
            onclick: "openTab( "People" )"
            "People"

    div.tab
        id: "Timeline"
        "This is the timeline."

        div < img.timeline#timeline
            src: "images\timeline.png"
            alt: "Timeline"
            width: "745px"
            height: "429px"
            usemap: "#timeline"

        map.timeline#timelineMap
            name: "timeline"

    div.tab
        id: "Events"
        div#eventcatbar
            button
                onclick: "searchEvents( "printAll", "" )"
                "All People"

            button#alphaFirstCat
                onclick: "genButtonList( "eventnav", "events", "", "alpha" )"
                "Search Alphabetically"

            button#categoryCat
                onclick: "genButtonList( "eventnav", "events", [ "Sexuality", "Gender", "Misc", "Trans" ], "general" )"
                "Search by Category"

            input#searchEvents
                type: "text"
                name: "searchbar"
                value: "Search Bar"
            button
                onclick: "searchEvents( "searchbar", "" )"
                "Search"
            
            ul#eventnav

        div#eventinfo < "Here are all the events alphabetically. Select a categorization method to narrow your search down further."
        table.eventTable#eventTable

    div.tab
        id: "Organizations"
        div#organizationcatbar
            button
                onclick: "searchOrganizations( "printAll", "" )"
                "All Organizations"

            button#alphaCat
                onclick: "genButtonList( "organizationnav", "organizations", "", "alpha" )"
                "Search Alphabetically"

            button#locationCat
                onclick: "genButtonList( "organizationnav", "organizations", [ "USA", "Czechoslovakia" ], "location" )"
                "Search by Location"

            button#dateCat
                onclick: "genButtonList( "organizationnav", "organizations", [ "1941 - 1945", "1220 - 1240", "2006 - 2050" ], "date" )"
                "Search by Date"

            input#searchOrganizations
                type: "text"
                name: "searchbar"
                value: "Search Bar"
            button
                onclick: "searchOrganizations( "searchbar", "" )"
                "Search"
            
            ul#organizationnav

        div#organizationInfo < "Here are all the organizations alphabetically. Select a categorization method to narrow your search down further."
        table.organizationTable#organizationTable

    div.tab
        id: "People"
        div#biocatbar
            button
                onclick: "searchBios( "printAll", "", true )"
                "All People"

            button#alphaFirstCat
                onclick: "genButtonList( "bionav", "biographies", "", "alpha1" )"
                "Search by First Name"

            button#alphaSecondCat
                onclick: "genButtonList( "bionav", "biographies", "", "alpha2" )"
                "Search by Last Name"

            button#categoryCat
                onclick: "genButtonList( "bionav", "biographies", [ "Sexuality", "Gender", "Misc", "Trans" ], "general" )"
                "Search by Category"

            input#searchText
                type: "text"
                name: "searchbar"
                value: "Search Bar"
            button
                onclick: "searchBios( "searchbar", "" )"
                "Search"
            
            ul#bionav

        div#bioinfo < "Here are all the people alphabetically by last name. Select a categorization method to narrow your search down further."
        table.bioTable#bioTable

    script
        clear( "tab" );
        loadTooltips();
        loadBios();
        loadEvents();
        loadOrganizations();
        genImageMapArea( false, [ 398, 222, 36, 203, 156, 191, 338, 196, 90, 163, 219, 148, 540, 92, 537, 119, 460, 264, 427, 208, 162, 293, 151, 84, 364, 129, 390, 131, 325, 112, 284, 155, 410, 155, 346, 250, 513, 20, 537, 133, 538, 70, 394, 89, 312, 53, 367, 215, 334, 158, 191, 50, 271, 120, 121, 129, 540, 56, 538, 107, 209, 214, 472, 72, 461, 167, 262, 46, 415, 118, 291, 192, 99, 74, 462, 97, 538, 80, 448, 189, 264, 82, 388, 177, 277, 240, 164, 141, 537, 40, 469, 138, 108, 34, 438, 137, 350, 80, 200, 97 ] );
        genButtonList( "bionav", "biographies", "", "alpha2" );
        searchBios( "printAll", "", true );
        checkForEnter( "bio" );
        genButtonList( "eventnav", "events", "", "alpha" );
        searchEvents( "printAll", "" );
        checkForEnter( "event" );
        genButtonList( "organizationnav", "organizations", "", "alpha" );
        searchOrganizations( "printAll", "" );
        checkForEnter( "organization" );