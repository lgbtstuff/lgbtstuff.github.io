p.content
    h1.map < "The Map"
    "Mouse over the abbreviation for any state to get more information."
    div
        img.map
            src: "images\map.gif"
            alt: "Legislation Map"
            width: "1118px"
            height: "644px"
            usemap: "#legislation"

        map.legislation
            name: "legislation"
            area#TX
                shape: "rect"
                coords: "0, 0, 60, 60"
                alt: "Texas"
                onmouseover: "tooltipInfo( "Texas" )"

        span
            div#tooltipHeader < "Texas"
            div#tooltip < "is real rude to all the gays."
