p.content
    nav.dict
        ul#dictnav(style: "list-style-type:none")
        "This is the dictionary!"
    div.terms
        table.termTable
            id: "termTable"
            tr < th
                colspan: "2"
                "Here are all the terms alphabetically. Select a categorization method to narrow your search down further."

    script
    	// Make a list of alphabetical buttons
        var alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
        genButtonList( "dictnav", "dictionary", alphabet.split("") );

        // Start the page with all the terms printed out
        dictDisplay( "printAll", "" );
