p.content
    nav.dict
        "This is the dictionary!"
        ul#dictnavalpha()
        ul#dictnavcat()
        ul#dictsearchbar()
            li < form < input#searchText
                type: "text"
                name: "searchbar"
                value: "Search for anything here!"
            li < button
                onclick: "searchDict( "searchbar", "" )"
                "Search"

    table.termTable
        id: "termTable"
        tr < th
            colspan: "2"
            "Here are all the terms alphabetically. Select a categorization method to narrow your search down further."

    script
        var alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
        genButtonList( "dictnavalpha", "dictionary", alphabet.split(""), "alpha" );
        var categories = [ "Sexuality", "Gender", "Misc", "Trans" ];
        genButtonList( "dictnavcat", "dictionary", categories, "general" );
        searchDict( "printAll", "" );
