p.content
    nav.dict
        h1 < "Dictionary"
        form
            input#alphaCat
                type: "radio"
                name: "Alphabetically"
                value: "Alphabetically"
                onclick: "genButtonList( "dictnav", "dictionary", "", "alpha" )"
            label
                for: "alphaCat"
                "Search by First Letter"

            input#categoryCat
                type: "radio"
                name: "Category"
                value: "Category"
                onclick: "genButtonList( "dictnav", "dictionary", [ "Sexuality", "Gender", "Misc", "Trans" ], "general" )"
            label
                for: "categoryCat"
                "Search by Category"

            input#searchText
                type: "text"
                name: "searchbar"
                value: "Search Bar"
            button
                onclick: "searchDict( "searchbar", "" )"
                "Search"
        ul#dictnav

    table.termTable
        id: "termTable"
        tr < th
            colspan: "2"
            "Here are all the terms alphabetically. Select a categorization method to narrow your search down further."

    script
        searchDict( "printAll", "" );
